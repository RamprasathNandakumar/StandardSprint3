  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select factcrime.State, 
sum (FactCrime.Recorded_Incidents) RecordedIncidents,
(SELECT SUM(FactCrime.Recorded_Incidents) FROM FactCrime) TotalCrime,
sum (FactCrime.Recorded_Incidents)*100/(SELECT SUM(FactCrime.Recorded_Incidents) FROM FactCrime) CrimeRate
from DimGeography
inner join FactCrime
on FactCrime.Suburb = DimGeography.Suburb
group by FactCrime.State</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="State">
          <DataField>State</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RecordedIncidents">
